{
  "name": "immutable-builtins",
  "version": "1.0.0",
  "description": "Dependency-free collection of immutable replacements for JavaScript builtins",
  "keywords": [
    "immutable",
    "immutability",
    "builtins",
    "readonly",
    "value-object",
    "date",
    "url",
    "urlsearchparams",
    "javascript",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francescosalvi/immutable-builtins.git"
  },
  "author": "Francesco Salvi <salvsuperb@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "postbuild:esm": "yarn exec tsc-esm-fix --target='target/es6'",
    "postbuild:cjs": "echo '{\"type\": \"commonjs\"}' > dist/cjs/package.json",
    "lint": "eslint .",
    "test": "jest",
    "ci": "yarn build && yarn lint && yarn test",
    "preversion": "yarn install --frozen-lockfile && yarn ci",
    "version": "node scripts/sync-jsr-version.js && git add jsr.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "24",
    "date-fns": "^4.1.0",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "tsc-esm-fix": "^3.1.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  }
}
